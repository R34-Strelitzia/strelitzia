{
  "extends": [
    "../../../.eslintrc.json",
    "plugin:@darraghor/nestjs-typed/recommended"
  ],
  "plugins": ["@darraghor/nestjs-typed"],
  "parserOptions": {
    "project": ["libs/backend/auth/tsconfig.*?.json"]
  },
  "rules": {
    "@darraghor/nestjs-typed/injectable-should-be-provided": "off"
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
